class RandomGenerator:
    def __init__(self, num_len):
        self.num_len = num_len

    def randgen(self):
        rand_num = self.__hash__()
        while len(str(rand_num)) < self.num_len:
                rand_num *= rand_num >> 2
        return int(str(rand_num)[:self.num_len])
if __name__ == "__main__":
    random_number = RandomGenerator(4)
    print(random_number.randgen())
    random_number = RandomGenerator(104)
    print(random_number.randgen())
    random_number = RandomGenerator(208)
    print(random_number.randgen())
    random_number = RandomGenerator(416)
    print(random_number.randgen())
    random_number = RandomGenerator(832)
    print(random_number.randgen())